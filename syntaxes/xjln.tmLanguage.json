{
	"name": "XJLN",
	"patterns": [
		{
			"include": "#keyword"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#char"
		},
		{
			"include": "#number"
		},
		{
			"include": "#operator"
		},
		{
			"include": "#identifier"
		}
	],
	"repository": {
		"keyword": {
			"name": "keyword.xjln",
			"match": "use|from|as|return|if|else|var|int|double|long|short|float|boolean|byte|char|def|class"
		},
		"strings": {
			"name": "string.xjln",
			"match": "\".*\""
		},
		"char": {
			"name": "char.xjln",
			"match": "'.'"
		},
		"number": {
			"name": "number.xjln",
			"match": "[0-9]+(\\.[0-9]+)?"
		},
		"operator": {
			"name": "operator.xjln",
			"match": "[-\\+\\*/%&\\|<>=!\\^]+"
		},
		"identifier": {
			"name": "identifier.xjln",
			"match": "[a-zA-Z][a-zA-Z0-9_]*"
		}
	},
	"scopeName": "source.xjln"
}